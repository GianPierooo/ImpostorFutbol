{
  "name": "impostor-futbol",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --reset-cache",
    "build:apk:debug": "npm run build:bundle:android && cd android && gradlew.bat clean && gradlew.bat assembleDebug && cd ..",
    "build:apk:release": "npm run build:bundle:android && cd android && gradlew.bat clean && gradlew.bat assembleRelease && cd ..",
    "version": "powershell -ExecutionPolicy Bypass -File ./generar-version.ps1",
    "version:v": "powershell -ExecutionPolicy Bypass -File ./generar-version.ps1"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/blur": "^4.4.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.6.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-animatable": "^1.4.0",
    "react-native-gesture-handler": "~2.18.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "~4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-vector-icons": "^10.3.0",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "^0.74.0",
    "@types/react": "^18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
